{
  "name": "hugo_site_css-grid",
  "description": "This is a Hugo starter based on https://brycewray.com.",
  "main": "postcss.config.js",
  "scripts": {
    "clean": "rm -rf public",
    "start": "npm-run-all clean --parallel dev:*",
    "dev:hugo": "hugo server",
    "build": "NODE_ENV=production npm-run-all clean --parallel prod:*",
    "prod:hugo": "hugo --gc --minify",
    "testbuild": "NODE_ENV=production npm-run-all clean --parallel testProd:*",
    "testProd:hugo": "hugo server"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "engines": {
    "node": "12.x"
  },
  "devDependencies": {
    "autoprefixer": "^10.2.4",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "from-env": "^1.1.4",
    "get-json": "^1.0.1",
    "jsonfile": "^6.1.0",
    "lazysizes": "^5.3.0",
    "lodash": "^4.17.21",
    "luxon": "^1.26.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.6",
    "postcss-clean": "^1.2.2",
    "postcss-cli": "^8.3.1",
    "postcss-css-variables": "^0.17.0",
    "postcss-import": "^14.0.0",
    "postcss-nesting": "^7.0.1",
    "postcss-partial-import": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^2.0.3"
  }
}
