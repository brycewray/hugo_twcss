{
  "name": "hugo_twcss",
  "description": "This is a Hugo starter based on https://brycewray.com.",
  "main": "postcss.config.js",
  "scripts": {
    "clean": "rm -rf public",
    "start": "npm-run-all clean --parallel dev:*",
    "dev:hugo": "hugo server",
    "build": "NODE_ENV=production npm-run-all clean --parallel prod:*",
    "prod:hugo": "hugo --gc --minify",
    "testbuild": "NODE_ENV=production npm-run-all clean --parallel testProd:*",
    "testProd:hugo": "hugo server"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "browserslist": [
    "last 4 versions",
    "> 1%",
    "maintained node versions"
  ],
  "devDependencies": {
    "autoprefixer": "^10.2.6",
    "cross-env": "^7.0.3",
    "from-env": "^1.1.4",
    "get-json": "^1.0.1",
    "jsonfile": "^6.1.0",
    "lazysizes": "^5.3.2",
    "lodash": "^4.17.21",
    "luxon": "^1.27.0",
    "node-fetch": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.1",
    "postcss-cli": "^8.3.1",
    "postcss-css-variables": "^0.18.0",
    "postcss-csso": "^5.0.1",
    "postcss-import": "^14.0.2",
    "postcss-nesting": "^8.0.1",
    "postcss-partial-import": "^4.1.0",
    "tailwindcss": "^2.1.4"
  }
}
